{
  "manifest_version": 2,

  "name": "cross-bookmarks",
  "description": "A Google Chrome extension to share bookmarks.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "image/icon.png",
    "default_popup": "html/index.html"
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ]
}
